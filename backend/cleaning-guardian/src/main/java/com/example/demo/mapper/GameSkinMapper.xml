<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.GameSkinMapper">

	<select id="getAllSkins" resultType="com.example.demo.vo.GameSkinsVO">
		SELECT *
		FROM "game_skins"
	</select>
	
	<select id="getSkin" resultType="com.example.demo.vo.GameSkinsVO">
		 SELECT * 
		 FROM "game_skins"
   		 WHERE "skin_idx" = #{skinIdx}
	</select>

</mapper>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Mapper.UserMapper">

<insert id="register" parameterType="com.example.demo.DTO.UserDTO">
INSERT INTO users (userLoginId) VALUES (#{userLoginId})
</insert>

<select id="UserLogin" resultType="java.lang.String">
SELECT userLoginId FROM users
</select>

<select id="UniqueGuestId" resultType="java.lang.String"> 
SELECT userLoginId FROM users
</select>

</mapper>